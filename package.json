{
  "name": "@onsite/fetch-cookie",
  "version": "2.2.0-rc.2",
  "description": "Decorator for a `fetch` function to support automatic cookies.",
  "license": "Unlicense",
  "author": "David Sargeant",
  "contributors": [
    "Val (https://val.codejam.info)"
  ],
  "type": "module",
  "private": true,
  "main": "./cjs/index-wrapper.js",
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index-wrapper.js"
      }
    }
  },
  "types": "esm/index.d.ts",
  "repository": "https://github.com/chorpler/fetch-cookie",
  "scripts": {
    "buildpreporiginal": "npm --prefix test/node_modules/node-fetch-2 ci && npm run patch-undici && npm run test && npm publish --dry-run",
    "buildprep": "npm --prefix test/node_modules/node-fetch-2 ci && npm run patch-undici",
    "build": "esbuild src/*.ts --format=esm --outdir=esm --target=node10.24 && esbuild src/*.ts --format=cjs --outdir=cjs --target=node10.24",
    "build:test": "esbuild test/*.js --format=cjs --outdir=test/testcjs --target=node10.24",
    "clean": "rimraf esm cjs && mkdirp esm cjs",
    "lint": "eslint src/*.ts test/*.js",
    "lintOriginal": "ts-standard src/*.ts test/*.js",
    "patch-undici": "sed -i.old 's/^const forbiddenResponseHeaderNames =.*$/const forbiddenResponseHeaderNames = []/' node_modules/undici/lib/fetch/constants.js",
    "prepack": "npm run prepare && npm run lint && npm run type-check && npm test && npm run test-import",
    "prepare": "npm run build && npm run type-declarations",
    "test": "npm run build && mocha",
    "test-import": "node test/cjs/import.js && ts-node test/cjs/import.ts",
    "type-check": "tsc -noEmit",
    "type-declarations": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir esm && cat esm/index.d.ts | sed 's/^export default /export = /' > cjs/index.d.ts"
  },
  "dependencies": {
    "set-cookie-parser": "^2.4.8",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.55",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/tough-cookie": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chai": "^4.3.6",
    "cross-fetch": "3.1.5",
    "esbuild": "^0.14.21",
    "eslint": "^8.21.0",
    "express": "^4.17.3",
    "mkdirp": "1.0.4",
    "mocha": "^9.2.0",
    "rimraf": "3.0.2",
    "ts-node": "^10.7.0",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.5",
    "undici": "^4.14.1"
  },
  "devDependenciesComments": {
    "removed": {
      "@onsite/node-fetch": "3.3.0-rc.1",
      "@types/node-fetch": "^2.6.2",
      "node-fetch": "^3.2.0"
    }
  }
}
